<script>
$(document).ready(function(){
  $('#mainav>ul.clear>li').removeClass('active');
  
  if ($(location).attr('href') == "{{ url('index') }}" || $(location).attr('href') == "{{ url('products') }}")
  {
    $('#mainav>ul.clear>li.products').addClass('active');
  }

  if ($(location).attr('href') == "{{ url('reservations') }}")
    {
    $('#mainav>ul.clear>li.reservations').addClass('active');
  }

  if ($(location).attr('href') == "{{ url('createproduct') }}")
  {
    $('#mainav>ul.clear>li.product').addClass('active');
  }

  if ($(location).attr('href') == "{{ url('createreservation') }}")
  {
    $('#mainav>ul.clear>li.reservation').addClass('active');
  }
});
</script>
